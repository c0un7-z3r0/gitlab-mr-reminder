    },
    "version": "0.0.1",
    "description": "Sends gitlab mr reminder to slack",
    "main": "src/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "build": "tsc",
        "watch": "concurrently \"tsc -w\" \"nodemon --watch dist dist/index.js\"",
