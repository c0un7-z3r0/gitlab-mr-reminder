    },
    "bin": {
        "gitlab-mr-reminder": "./dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/c0un7-z3r0/gitlab-mr-reminder.git"
    },
    "keywords": [
        "gitlab",
        "git",
        "slack",
        "reminder"
    ],
    "license": "MIT",
    "dependencies": {
        "@slack/webhook": "^5.0.2",
        "got": "^11.8.5"
    },
    "devDependencies": {
        "@types/got": "^9.6.7",
        "concurrently": "^5.0.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.4",
        "prettier": "1.18.2",
        "typescript": "^3.6.3"
    }
}
